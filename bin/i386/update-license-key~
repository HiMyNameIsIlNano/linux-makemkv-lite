#!/bin/sh

# Start functions
# Print out the help
usage(){
    echo "usage: update-license-keys | [[-h]]"
}
# End functions 

while [ "$1" != "" ]; do
    case $1 in
        -s | --source-folder )	shift
                                SRC_FOLDER=$1
                                ;;
        -h | --help )           usage
                                exit
                                ;;
        * )                     usage
                                exit 1
    esac
    shift
done


